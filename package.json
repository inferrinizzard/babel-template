{
	"name": "babel-template",
	"version": "0.1.0",
	"description": "Template for React + TS, Webpack, Babel",
	"author": "Sean Song",
	"license": "MIT",
	"repository": "https://github.com/inferrinizzard/babel-template.git",
	"main": "index.tsx",
	"scripts": {
		"clean": "rimraf dist/*",
		"lint": "eslint --quiet --ext .js,.jsx,.ts,.tsx --ignore-pattern *.ejs ./src/**/*",
		"fmt": "prettier \"./src/**/*.{js,jsx,ts,tsx}\" --write",
		"build": "yarn run clean-dist && webpack --config=webpack.config.prod.js",
		"build:dev": "yarn run clean-dist && webpack --config webpack.config.dev.js",
		"start": "webpack serve --config=webpack.config.dev.js",
		"start:prod": "yarn run build && node express.js"
	},
	"pre-commit": [
		"lint",
		"fmt"
	],
	"devDependencies": {
		"@babel/cli": "^7.16.0",
		"@babel/core": "^7.16.0",
		"@babel/preset-env": "^7.16.0",
		"@babel/preset-react": "^7.16.0",
		"@babel/preset-typescript": "^7.16.0",
		"@types/express": "^4.17.13",
		"@types/node": "^16.11.6",
		"@types/react": "^17.0.33",
		"@types/react-dom": "^17.0.10",
		"@types/react-hot-loader": "^4.1.1",
		"@typescript-eslint/eslint-plugin": "^5.3.0",
		"@typescript-eslint/parser": "^5.3.0",
		"babel-loader": "^8.2.3",
		"css-loader": "^6.5.0",
		"eslint": "^8.1.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.26.1",
		"express": "^4.17.1",
		"file-loader": "^6.2.0",
		"html-webpack-plugin": "^5.5.0",
		"image-webpack-loader": "^8.0.1",
		"pre-commit": "^1.2.2",
		"prettier": "^2.4.1",
		"react-hot-loader": "^4.13.0",
		"rimraf": "^3.0.2",
		"style-loader": "^3.3.1",
		"typescript": "^4.4.4",
		"webpack": "^5.61.0",
		"webpack-cli": "^4.9.1",
		"webpack-dev-server": "^4.4.0",
		"webpack-merge": "^5.8.0"
	},
	"dependencies": {
		"react": "^17.0.2",
		"react-dom": "npm:@hot-loader/react-dom"
	}
}
